#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

int years,weeks,days;
int total_days;
    scanf("%d",&total_days);
    years=total_days/365;
    total_days=total_days%365;
    weeks=total_days/7;
    days=total_days%7;
    printf("Years: %d\nWeeks: %d\nDays: %d\n",years,weeks,days);
    return 0;
}
